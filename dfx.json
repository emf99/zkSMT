{
  "canisters": {
    "ss1_backend": {
      "candid": "src/ss1_backend/ss1_backend.did",
      "package": "ss1_backend",
      "type": "rust"
    },
    "ss1_frontend": {
      "dependencies": [
        "ss1_backend"
      ],
      "source": [
        "src/ss1_frontend/dist"
      ],
      "type": "assets",
      "workspace": "ss1_frontend",
      "headers": {
        "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-eval' 'wasm-unsafe-eval'; worker-src 'self' blob:; object-src 'none';"
      }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}